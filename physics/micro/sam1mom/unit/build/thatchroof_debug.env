#!/bin/bash

unset YAKL_ARCH

export YAKL_CXX_FLAGS="-O0 -g -DYAKL_DEBUG -DPAM_DEBUG -I/opt/netcdf_gnu/include"
export YAKL_C_FLAGS="-O0 -g"
export YAKL_F90_FLAGS="-ffree-line-length-none -O0 -g -fcheck=all"
export NF_INCLUDE="/usr/include"
export NF_LIBS="`/usr/bin/nf-config --flibs`"

unset CXXFLAGS
unset FFLAGS
unset F77FLAGS
unset F90FLAGS
