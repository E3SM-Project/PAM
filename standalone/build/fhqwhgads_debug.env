#!/bin/bash

unset YAKL_ARCH

export YAKL_HOME=/home/${USER}/YAKL
export YAKL_CXX_FLAGS="-O0 -g -DYAKL_DEBUG -DPAM_DEBUG --std=c++11"
unset CXXFLAGS
export FFLAGS="-O0 -g -DYAKL_DEBUG -DPAM_DEBUG"
export F77FLAGS="-O0 -g -DYAKL_DEBUG -DPAM_DEBUG"
export F90FLAGS="-O0 -g -DYAKL_DEBUG -DPAM_DEBUG"
export NCFLAGS="-lnetcdf_c++4"

